{
  "name": "@ricado/version-file",
  "version": "1.0.2",
  "description": "Generates a verson file with your apps name, version and build date",
  "repository": {
    "url": "https://github.com/ricado-group/js-version-file.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "heft test --clean",
    "test": "node ./node_modules/mocha/bin/mocha test/index.js",
    "docs": "api-documenter generate --input-folder ./temp/api --output-folder ./docs"
  },
  "keywords": [
    "version",
    "file",
    "build",
    "date"
  ],
  "license": "MIT",
  "bin": {
    "generate-version": "./bin/cli.js"
  },
  "dependencies": {
    "@rushstack/node-core-library": "^3.37.0",
    "chalk": "3.0.0",
    "ejs": "2.7.2",
    "meow": "5.0.0",
    "underscore": "1.9.1"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.13.4",
    "@rushstack/eslint-config": "^2.3.4",
    "@rushstack/heft": "^0.30.2",
    "@rushstack/heft-node-rig": "^1.0.18",
    "@types/ejs": "^3.0.6",
    "@types/heft-jest": "^1.0.1",
    "@types/node": "^12.20.11",
    "@types/underscore": "^1.11.2",
    "typescript": "^3.9.9"
  }
}
